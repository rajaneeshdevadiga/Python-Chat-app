{
  "family": "chat-app",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "chat-app",
      "image": "730335489777.dkr.ecr.us-east-1.amazonaws.com/chat-app:latest",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 5000, 
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ],
  "requiresCompatibilities": [
    "FARGATE"  
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::730335489777:role/ecs-task",  
  "taskRoleArn": "arn:aws:iam::730335489777:role/ecs-task-full"  
}
